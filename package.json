{
  "name": "pb2ts",
  "version": "1.0.0",
  "description": "Protocol Buffers to TypeScript code generator - 从 Protocol Buffers 生成 TypeScript 代码的工具",
  "main": "dist/main.js",
  "bin": {
    "pb2ts": "./dist/main.js",
    "pb2ts-parser": "./bin/pb2ts-parser"
  },
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "build": "node scripts/build.js && node scripts/tsbuild.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "protobuf",
    "protocol-buffers",
    "typescript",
    "code-generator",
    "grpc",
    "api-generator"
  ],
  "author": "",
  "license": "MulanPSL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/adminck/pb2ts.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "files": [
    "bin",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/node": "^25.0.3",
    "typescript": "^5.6.3"
  }
}
